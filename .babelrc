{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "16"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-flow",
    "@babel/transform-flow-strip-types",
    "@babel/plugin-proposal-export-default-from"
  ],
  "sourceMaps": "inline"
}
